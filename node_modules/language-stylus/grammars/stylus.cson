'name': 'Stylus'
'patterns': [
  {
    'captures':
      '1':
        'name': 'punctuation.definition.comment.stylus'
    'match': '(?:^[ \\t]+)?(\\/\\/).*$\\n?'
    'name': 'comment.line.stylus'
  }
  {
    'begin': '/\\*'
